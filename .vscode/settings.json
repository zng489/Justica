{
  "prettier.singleQuote": true,
  "files.exclude": {
    ".vscode/chrome/**/*": true
  },
  "search.exclude": {
    ".vscode/chrome/**/*": true,
    "dist/**/*": true,
    "www/**/*": true,
    "platforms/**/*": true,
    "node_modules/**/*": true
  },
  "angulardoc.repoId": "06230efc-c885-4a09-86b4-765379786a5f",
